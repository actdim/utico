{
    "name": "@actdim/utico",
    "version": "0.9.3",
    "description": "A modern foundation toolkit for complex TypeScript apps",
    "author": "Pavel Borodaev",
    "license": "Proprietary",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/actdim/utico.git"
    },
    "bugs": {
        "url": "https://github.com/actdim/utico/issues"
    },
    "homepage": "https://github.com/actdim/utico/#readme",
    "keywords": [
        "core",
        "common",
        "typescript",
        "utility",
        "utils"
    ],
    "files": [
        "dist",
        "docs",
        "README.md",
        "LICENSE"
    ],
    "files(src)": [
        "src",
        "docs",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        "./*": {
            "import": "./dist/*.es.js",
            "types": "./dist/*.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "sideEffects?": [],
    "scripts": {
        "test": "npx vitest",
        "wtest": "npx vitest --watch",
        "dtest": "node --inspect-brk node_modules/vitest/vitest.mjs run --poolOptions.threads.singleThread",
        "lint": "eslint . -f visualstudio --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
        "online": "vite --host",
        "build": "tsc -b && vite build",
        "npm:u": "npm update",
        "npm:ou": "npm outdated",
        "ncu": "ncu",
        "ncu:u": "ncu -u",
        "ncu:ud": "ncu -u --dev",
        "nvm:la": "nvm list available",
        "nvm:il": "nvm install lts"
    },
    "peerDependencies": {
        "dexie": "^4.2.0",
        "moment": "^2.30.1",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@swc/core": "^1.13.5",
        "@types/lodash": "^4.17.20",
        "@types/node": "^24.7.0",
        "@types/uuid": "^11.0.0",
        "@typescript-eslint/eslint-plugin": "^8.45.0",
        "@typescript-eslint/parser": "^8.45.0",
        "@vitejs/plugin-react-swc": "^4.1.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-formatter-visualstudio": "^8.40.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^6.1.1",
        "eslint-plugin-react-refresh": "^0.4.23",
        "globals": "^16.4.0",
        "npm-check-updates": "^19.0.0",
        "prettier": "^3.6.2",
        "prettier-plugin-classnames": "^0.8.4",
        "shx": "^0.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.9",
        "vite-plugin-dts": "^4.5.4",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    }
}
